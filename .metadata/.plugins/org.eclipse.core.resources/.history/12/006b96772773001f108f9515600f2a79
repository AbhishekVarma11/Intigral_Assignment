package RestRequests;
	import lombok.AllArgsConstructor;
	import lombok.Data;
	import lombok.NoArgsConstructor;

	@Data
	@NoArgsConstructor
	@AllArgsConstructor
	public class Device {
	    private String id;
	    private String name;
	    private Data data;

	    // Constructor (for creating request body without 'id')
	    public Device(String name, Data data) {
	        this.name = name;
	        this.data = data;
	    }
	}

